    "# config/logger_config.py\n",
    "import logging\n",
    "import sys\n",
    "import CONFIG\n",
    "\n",
    "def setup_logger():\n",
    "    logging.basicConfig(stream=sys.stdout, level=logging.INFO)\n",
    "    logger = logging.getLogger()\n",
    "    logger.addHandler(logging.StreamHandler(stream=sys.stdout))\n",
    "    return logger"
